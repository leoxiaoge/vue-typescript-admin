    "vue-html-loader": "^1.2.4",
    "vue-improve-loader": "^0.1.1",
    "vue-jest": "^1.0.2",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
